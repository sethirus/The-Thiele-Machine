# RIEMANN STRATEGY: Wide Grid Scan
# Broad search across critical strip

STRATEGY riemann_grid
VERSION 1.0
DESCRIPTION "Wide grid search in critical strip"

SEQUENCE:
  candidates = GRID_SEARCH(0.4, 0.6, 14.0, 100.0, 0.5)
  for point in candidates:
    refined, mag = REFINE_ZERO(point)
    if mag < 1e-10:
      verification = VERIFY_COUNTEREXAMPLE(refined)

METADATA:
  primitive_count: 4
  complexity: MEDIUM
  basis: "Grid-based search"
  generation: 0
  fitness: 0.0
